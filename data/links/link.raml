#%RAML 1.0 Library
uses:
  User: !include ../users/user.raml
  common: !include ../common/dataTypes.raml
types:
  Link:
    type: object
    example: !include link.json
    properties:
      link_id: string
      url: string
      added_by: User.User
      date_added: common.Date
      votes_counter:
        type: object
        properties:
          up: number
          down: number
      tags:
        type: string[]
        uniqueItems: true
