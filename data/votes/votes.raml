#%RAML 1.0 Library
uses:
  common: ../common/dataTypes.raml
  link: ../links/link.raml
  user: ../users/user.raml
types:
  Vote:
    type: object
    properties:
      grade:
        type: string
        enum:
          - up
          - down
      date_voted: common.Date
  LinkVote:
    type: Vote
    properties:
      user: user.User
  UserVote:
    type: Vote
    example: !include userVote.json
    properties:
      link: link.Link
